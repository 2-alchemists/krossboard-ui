{
  "name": "kube-opex-analytics-mc-frontend",
  "version": "1.0.0",
  "description": "Frontend for kube-opex-analytics-mc",
  "author": "(c) Company 2019",
  "license": "UNLICENSED",
  "main": "src/index.tsx",
  "scripts": {
    "develop": "concurrently -r \"yarn:type-check:watch\" \"yarn:start\"",
    "start": "parcel ./src/index.html",
    "build": "yarn type-check && parcel build ./src/index.html --no-source-maps",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "start-server": "nodemon server/index.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'src/**/*.tsx' 'test/**/*.ts'"
  },
  "engines": {
    "node": ">= 10.19.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/styleMock.ts"
    }
  },
  "keywords": [
    "Kubernetes",
    "Analytics",
    "Cluster"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.5.2",
    "@material-ui/pickers": "^3.2.8",
    "@types/d3": "^5.7.2",
    "@types/d3-time-format": "^2.1.1",
    "@types/react-resize-detector": "^4.0.2",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.2",
    "@types/recharts": "^1.8.4",
    "@use-it/interval": "^0.1.3",
    "axios": "^0.19.1",
    "clsx": "^1.0.4",
    "cors": "^2.8.5",
    "d3-selection": "^1.4.0",
    "d3-time-format": "^2.2.3",
    "date-fns": "^2.9.0",
    "dist": "^0.1.2",
    "express": "^4.17.1",
    "mobx": "^5.15.2",
    "mobx-react-lite": "^1.5.2",
    "prop-types": ">=15",
    "react": ">=15",
    "react-dom": ">=15",
    "react-resize-detector": "^4.2.1",
    "react-router-dom": "^5.1.2",
    "recharts": "1.8.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^11.13.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "concurrently": "^4.1.0",
    "husky": "^4.2.1",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-clean-dist": "^0.0.6",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.4.4"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
     }
  }
}
