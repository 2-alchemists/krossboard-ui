{
  "name": "kube-opex-analytics-mc-frontend",
  "version": "1.0.0",
  "description": "Frontend for kube-opex-analytics-mc",
  "author": "(c) Company 2019",
  "license": "UNLICENSED",
  "main": "src/index.tsx",
  "scripts": {
    "develop": "concurrently -r \"yarn:type-check:watch\" \"yarn:start\"",
    "start": "parcel ./src/index.html",
    "build": "yarn type-check && parcel build ./src/index.html --no-source-maps",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'src/**/*.tsx' 'test/**/*.ts'"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/styleMock.ts"
    }
  },
  "keywords": [
    "Kubernetes",
    "Analytics",
    "Cluster"
  ],
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@types/d3": "^5.7.2",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.2",
    "britecharts": "2.11.0",
    "britecharts-react": "^0.5.3",
    "d3-selection": "^1.4.0",
    "dist": "^0.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^11.13.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "concurrently": "^4.1.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-clean-dist": "^0.0.6",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.4"
  }
}
